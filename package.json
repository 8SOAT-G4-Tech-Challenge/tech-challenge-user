	"description": "API Restaurante FIAP Tech Challenge",
	"main": "index.js",
	"scripts": {
		"build": "tsup src",
		"dev": "tsx watch --env-file .env -r tsconfig-paths/register src/adapter/driver/server.ts | pino-pretty -C",
		"dev:no-env": "tsx watch -r tsconfig-paths/register src/adapter/driver/server.ts",
		"lint": "eslint src --ext .ts --fix",
		"db:migrate": "prisma migrate dev",
		"db:studio": "prisma studio",
		"db:seed": "prisma db seed",
		"db:clean": "tsx prisma/clean.ts",
		"db:reset": "npm run db:clean && npm run db:seed"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"prisma": {
		"seed": "tsx prisma/seed.ts",
		"schema": "./prisma/schema.prisma"
	},
	"dependencies": {
		"@fastify/cors": "^9.0.1",
		"@fastify/helmet": "^11.1.1",
		"@fastify/multipart": "^8.3.0",
		"@fastify/swagger": "^8.14.0",
		"@fastify/swagger-ui": "^4.0.0",
		"@prisma/client": "^5.17.0",
		"axios": "^1.7.7",
		"dotenv": "^16.4.5",
		"fastify": "^4.28.1",
		"fastify-type-provider-zod": "^2.0.0",
		"http-status-codes": "^2.3.0",
		"pino": "^9.3.2",
		"redis": "^4.7.0",
		"tsconfig-paths": "^4.2.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/node": "^20.14.10",
		"@types/pino": "^7.0.5",
		"@typescript-eslint/eslint-plugin": "^7.15.0",
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^8.57.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import-helpers": "^1.3.1",
		"pino-pretty": "^11.2.2",
		"prisma": "^5.17.0",
		"tsup": "^8.1.0",
		"tsx": "^4.16.2",
		"typescript": "^5.5.3"
	}
